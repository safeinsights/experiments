{
  "name": "safeinsightsaicompanion",
  "displayName": "SafeInsights AI Companion",
  "description": "AI-powered code assistant for safe and insightful development",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.74.0"
  },
  "categories": ["Other"],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "safeinsightsaicompanion.openChat",
        "title": "Open SafeInsights AI Chat"
      }
    ],
    "configuration": {
      "title": "SafeInsights AI Companion",
      "properties": {
        "safeinsightsaicompanion.apiKey": {
          "type": "string",
          "default": "",
          "description": "Your Together.ai API key"
        },
        "safeinsightsaicompanion.model": {
          "type": "string",
          "default": "meta-llama/Llama-3-70b-chat-hf",
          "description": "The model to use for chat"
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./"
  },
  "devDependencies": {
    "@types/vscode": "^1.74.0",
    "@types/node": "16.x",
    "typescript": "^4.9.4"
  },
  "dependencies": {
    "axios": "^1.6.0"
  }
}